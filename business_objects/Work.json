            ]
        },
        "address": {
            "type": "string",
            "default": "Campo Grande 016, 1749-016 Lisboa"
        },
        "contractor_descriptor": {
            "type": "string"
        },
        "contractor_contact": {
            "type": "string"
        }
    }
}